[Desktop Entry]
_Name=@APP_NAME@
_Comment=@RELEASE_NAME@
TryExec=@EXEC_NAME@
Exec=@EXEC_NAME@ %U
Icon=@DESKTOP_ICON@
_Keywords=Email;E-mail;Mail;
Terminal=false
Type=Application
MimeType=x-scheme-handler/mailto;
StartupNotify=true
Categories=GNOME;GTK;Network;Email;
X-GNOME-Gettext-Domain=@GETTEXT_PACKAGE@
X-GNOME-UsesNotifications=true
Actions=AboutDialog;Compose;

[Desktop Action Compose]
_Name=Compose Message
Exec=@EXEC_NAME@ mailto:

[Desktop Action AboutDialog]
_Name=About @APP_NAME@
Exec=@EXEC_NAME@ --about
